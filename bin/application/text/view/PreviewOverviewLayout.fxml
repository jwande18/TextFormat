<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="480.0" prefWidth="820.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.text.view.PreviewOverviewController">
   <children>
      <HBox prefHeight="480.0" prefWidth="820.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <AnchorPane prefHeight="480.0" prefWidth="630.0">
               <children>
                  <VBox prefHeight="480.0" prefWidth="630.0" spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <children>
                        <Label alignment="CENTER" prefWidth="630.0" text="Preview" />
                        <TextArea fx:id="previewArea" prefHeight="350.0" prefWidth="630.0" promptText="Text Format Result...">
                           <font>
                              <Font name="Courier New" size="12.0" />
                           </font></TextArea>
                        <Label alignment="CENTER" prefWidth="630.0" text="Status &amp; Error Log" />
                        <TextArea fx:id="statusArea" prefHeight="200.0" prefWidth="630.0" promptText="Error Report History...">
                           <font>
                              <Font name="Courier New Italic" size="12.0" />
                           </font></TextArea>
                     </children>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </VBox>
               </children>
            </AnchorPane>
            <AnchorPane prefHeight="480.0" prefWidth="190.0">
               <children>
                  <VBox prefHeight="480.0" prefWidth="190.0" spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <children>
                        <Label alignment="CENTER" prefWidth="180.0" text="Controls" />
                        <Button mnemonicParsing="false" onAction="#loadFile" prefWidth="180.0" text="Load" />
                        <Button mnemonicParsing="false" onAction="#saveFile" prefWidth="180.0" text="Save" />
                        <Button mnemonicParsing="false" onAction="#processFile" prefWidth="180.0" text="Preview" />
                        <AnchorPane prefHeight="404.0" prefWidth="180.0">
                           <children>
                              <Button mnemonicParsing="false" onAction="#exitApplication" prefWidth="180.0" text="Exit" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" />
                           </children>
                        </AnchorPane>
                     </children>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </VBox>
               </children>
            </AnchorPane>
         </children>
      </HBox>
   </children>
</AnchorPane>
